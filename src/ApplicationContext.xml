<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
    http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">


<bean id="miInforme" class="com.andres.maven.ProyectoSpringUno.Informe"></bean>

<bean id="miEmpleado" class="com.andres.maven.ProyectoSpringUno.JefeEmpleado"><constructor-arg ref="miInforme"></constructor-arg>
<property name="email" value="empleado@pildorasinformaticas.es"></property>
<property name="nombreEmpresa" value="pildorasinformaticas"></property>
</bean>

<bean id="miSecretarioEmpleado" class="com.andres.maven.ProyectoSpringUno.SecretarioEmpleado">
<property name="informeNuevo" ref="miInforme"></property>
<property name="email" value="secretario@pildorasinformaticas.es"></property>
<property name="nombreEmpresa" value="pildorasinformaticas"></property>
</bean>

</beans>